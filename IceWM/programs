## This is an example for icewm 3.3.3 program definition file.
## It will be installed as /usr/share/icewm/programs.
## Place your variants in /etc/icewm or in $HOME/.icewm.
## This file is intended to be customized by distributions.
##
## mostly obsolete, fixme

menu Acessórios folder {
	prog "KeePassXC" keepassxc keepassxc
	prog "Calculadora" calculator mate-calc
}

menu Internet folder {
	prog "Telegram Desktop" telegram telegram-desktop
	prog "Firefox" firefox firefox
}

menu Multimídia folder {
	prog "MPV Player" mpv mpv --player-operation-mode=pseudo-gui
	prog "Audacious" audacious audacious
	prog "KDEnlive" kdenlive kdenlive
}

menu Jogos folder {
	prog Steam steam steam
	prog Retroarch applications-games flatpak run org.libretro.Retroarch
}

menu Configurações folder {
	prog "Configuração do LightDM" lightdm-gtk-greeter-settings lightdm-gtk-greeter-settings-pkexec 
}

